import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.println("Введите три разных числа");
        int a= scan.nextInt();
        int b= scan.nextInt();
        int c= scan.nextInt();
        System.out.println("Произведение трех чисел="+ a*b*c);
        }
    }
